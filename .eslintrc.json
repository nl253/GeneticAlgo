{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jest": true,
    "node": true,
    "shared-node-browser": true,
    "worker": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "plugin:security/recommended",
    "plugin:promise/recommended",
    "airbnb"
  ],
  "globals": {
    "Array": true,
    "Date": true,
    "JSON": true,
    "Map": true,
    "Math": true,
    "Number": true,
    "Object": true,
    "Promise": true,
    "Set": true,
    "URL": true,
    "document": true,
    "module": true,
    "navigator": true,
    "process": true,
    "window": true
  },
  "parserOptions": {
    "codeFrame": false,
    "ecmaFeatures": {
      "impliedStrict": true,
      "jsx": true
    },
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "plugins": [
    "optimize-regex",
    "filenames",
    "promise",
    "compat",
    "security"
  ],
  "root": true,
  "rules": {
    "accessor-pairs": [
      2,
      {
        "getWithoutSet": true
      }
    ],
    "array-bracket-newline": [
      1,
      {
        "minItems": 4,
        "multiline": true
      }
    ],
    "array-callback-return": 1,
    "array-element-newline": [
      1,
      {
        "minItems": 4,
        "multiline": true
      }
    ],
    "arrow-spacing": [
      1,
      {
        "after": true,
        "before": true
      }
    ],
    "block-scoped-var": 1,
    "callback-return": 1,
    "camelcase": 1,
    "capitalized-comments": [
      1,
      "never",
      {
        "block": {
          "ignorePattern": ".*"
        }
      }
    ],
    "class-methods-use-this": 1,
    "complexity": [
      1,
      5
    ],
    "consistent-return": 1,
    "consistent-this": 1,
    "constructor-super": 2,
    "curly": [
      2,
      "multi-line"
    ],
    "default-case": 1,
    "dot-location": [
      2,
      "property"
    ],
    "dot-notation": 1,
    "eol-last": 2,
    "filenames/match-exported": 0,
    "filenames/match-regex": 2,
    "filenames/no-index": 0,
    "for-direction": 2,
    "generator-star-spacing": [
      1,
      {
        "after": true,
        "before": false
      }
    ],
    "getter-return": 2,
    "global-require": 1,
    "guard-for-in": 0,
    "handle-callback-err": [
      2,
      "^(err|error)$"
    ],
    "init-declarations": 0,
    "lines-around-comment": [
      1,
      {
        "afterBlockComment": false,
        "allowArrayEnd": false,
        "allowArrayStart": false,
        "allowBlockStart": true,
        "allowObjectEnd": true
      }
    ],
    "lines-between-class-members": 1,
    "max-depth": [
      1,
      3
    ],
    "max-lines": [
      1,
      120
    ],
    "max-nested-callbacks": [
      1,
      3
    ],
    "max-params": [
      1,
      5
    ],
    "max-statements": [
      1,
      300
    ],
    "max-statements-per-line": 1,
    "multiline-comment-style": 1,
    "multiline-ternary": 1,
    "new-parens": 2,
    "no-alert": 1,
    "no-await-in-loop": 1,
    "no-bitwise": 1,
    "no-buffer-constructor": 1,
    "no-caller": 2,
    "no-catch-shadow": 1,
    "no-class-assign": 2,
    "no-compare-neg-zero": 2,
    "no-cond-assign": 2,
    "no-console": 0,
    "no-const-assign": 2,
    "no-constant-condition": [
      2,
      {
        "checkLoops": false
      }
    ],
    "no-continue": 1,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-div-regex": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-duplicate-imports": 2,
    "no-else-return": 0,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-function": 2,
    "no-empty-pattern": 2,
    "no-eq-null": 1,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-label": 2,
    "no-extra-parens": [
      2,
      "functions"
    ],
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-global-assign": 2,
    "no-implicit-coercion": 1,
    "no-implicit-globals": 0,
    "no-implied-eval": 2,
    "no-inline-comments": 0,
    "no-inner-declarations": [
      2,
      "functions"
    ],
    "no-invalid-regexp": 2,
    "no-invalid-this": 1,
    "no-irregular-whitespace": 2,
    "no-label-var": 2,
    "no-labels": [
      2,
      {
        "allowLoop": false,
        "allowSwitch": false
      }
    ],
    "no-lone-blocks": 2,
    "no-lonely-if": 1,
    "no-loop-func": 2,
    "no-magic-numbers": [
      1,
      {
        "ignore": [
          0,
          1,
          2,
          -1,
          -2,
          10,
          100
        ],
        "ignoreArrayIndexes": true
      }
    ],
    "no-mixed-requires": 1,
    "no-mixed-operators": 0,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-negated-condition": 0,
    "no-negated-in-lhs": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-new-symbol": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-param-reassign": 2,
    "no-path-concat": 2,
    "no-plusplus": 0,
    "max-len": 0,
    "no-process-env": 2,
    "no-process-exit": 2,
    "no-proto": 2,
    "no-prototype-builtins": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-restricted-syntax": [
      2,
      "WithStatement"
    ],
    "no-return-assign": [
      2,
      "except-parens"
    ],
    "no-return-await": 2,
    "no-script-url": 1,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": [
      2,
      {
        "builtinGlobals": false,
        "hoist": "functions"
      }
    ],
    "no-shadow-restricted-names": 2,
    "no-sparse-arrays": 2,
    "no-sync": [
      1,
      {
        "allowAtRootLevel": true
      }
    ],
    "no-tabs": 2,
    "no-template-curly-in-string": 2,
    "no-ternary": 1,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-underscore-dangle": [
      "error",
      {
        "allowAfterThis": true
      }
    ],
    "no-unexpected-multiline": 2,
    "no-unmodified-loop-condition": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 2,
    "no-unused-expressions": [
      2,
      {
        "allowShortCircuit": true,
        "allowTaggedTemplates": true,
        "allowTernary": true
      }
    ],
    "no-unused-labels": 1,
    "no-unused-vars": [
      2,
      {
        "args": "none",
        "ignoreRestSiblings": true,
        "vars": "all"
      }
    ],
    "no-use-before-define": [
      2,
      {
        "classes": false,
        "functions": false,
        "variables": false
      }
    ],
    "no-useless-call": 2,
    "no-useless-computed-key": 2,
    "no-useless-concat": 1,
    "no-useless-escape": 2,
    "no-useless-rename": 2,
    "no-useless-return": 2,
    "no-var": 1,
    "no-void": 2,
    "no-warning-comments": 0,
    "no-with": 2,
    "object-curly-newline": 0,
    "object-property-newline": [
      2,
      {
        "allowMultiplePropertiesPerLine": true
      }
    ],
    "one-var-declaration-per-line": 1,
    "operator-assignment": 1,
    "optimize-regex/optimize-regex": 1,
    "padding-line-between-statements": 1,
    "prefer-arrow-callback": 1,
    "prefer-const": 1,
    "prefer-destructuring": 1,
    "prefer-numeric-literals": 1,
    "prefer-promise-reject-errors": 2,
    "prefer-rest-params": 1,
    "prefer-spread": 1,
    "prefer-template": 1,
    "quote-props": [
      1,
      "consistent-as-needed"
    ],
    "quotes": [
      2,
      "single",
      {
        "allowTemplateLiterals": true,
        "avoidEscape": true
      }
    ],
    "require-await": 2,
    "require-jsdoc": 1,
    "require-yield": 1,
    "rest-spread-spacing": [
      2,
      "never"
    ],
    "semi-spacing": [
      2,
      {
        "after": true,
        "before": false
      }
    ],
    "sort-imports": 1,
    "sort-keys": 1,
    "sort-vars": 0,
    "space-unary-ops": [
      2,
      {
        "nonwords": false,
        "words": true
      }
    ],
    "switch-colon-spacing": 1,
    "symbol-description": 2,
    "template-curly-spacing": [
      2,
      "never"
    ],
    "template-tag-spacing": [
      2,
      "never"
    ],
    "unicode-bom": [
      2,
      "never"
    ],
    "use-isnan": 2,
    "valid-jsdoc": [
      1,
      {
        "matchDescription": ".+",
        "prefer": {
          "arg": "param",
          "argument": "param",
          "augments": "extends",
          "class": "constructor",
          "constant": "const",
          "defaultValue": "default",
          "description": "desc",
          "exception": "throws",
          "file": "overview",
          "fileoverview": "overview",
          "function": "func",
          "host": "external",
          "linkcode": "link",
          "linkplain": "link",
          "property": "prop",
          "return": "returns",
          "virtual": "abstract",
          "yield": "yields"
        },
        "preferType": {
          "Boolean": "boolean",
          "Number": "number",
          "String": "string",
          "object": "Object"
        },
        "requireParamDescription": false,
        "requireReturn": false,
        "requireReturnDescription": false,
        "requireReturnType": true
      }
    ],
    "valid-typeof": [
      2,
      {
        "requireStringLiterals": true
      }
    ],
    "vars-on-top": 1,
    "wrap-regex": 0,
    "yield-star-spacing": [
      2,
      {
        "after": false,
        "before": true
      }
    ],
    "yoda": [
      2,
      "never"
    ]
  }
}
