  "scripts": {
    "build": "npx tsc -p .",
    "watch": "npx tsc -p . --watch",
    "build:examples": "npx tsc -p ./tsconfig.example.json",
    "example:simple": "node ./examples/simple.js",
    "example:math": "node ./examples/math.js",
    "example:parallel": "node ./examples/parallel.js",
    "example:multiobjective": "node ./examples/multiobjective.js",
    "test": "npx jest",
    "doc": "echo -e 'GENERATING DOCS\\n'; rm -rf ./docs; npx typedoc --readme ./README.md --out ./docs",
    "doc:publish": "echo -e 'PUBLISHING DOCS\\n'; npm run doc; git add ./docs; git commit -am 'Update docs' -n; npm version patch; git push",
    "doc:open": "npm run doc; $BROWSER ./docs/index.html",
    "lint": "npx eslint *.ts",
    "loc": "tokei -- $(git ls-files | grep -E '\\.js$' | grep -v docs | grep -v bin)",
    "pretest": "npm run build",
    "prepublishOnly": "npm run build; npm run doc"
  }
